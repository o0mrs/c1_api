{
  "name": "@onehop/js",
  "version": "1.3.11",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module",
  "repository": "https://github.com/hopinc/hop-js.git",
  "homepage": "https://js.hop.io",
  "author": "Hop <developers@hop.io>",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "yarn@3.2.1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json",
    "utils"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup",
    "bench": "HOP_DEBUG=true tsx _workbench.ts",
    "release": "yarn build && yarn npm publish && rm -rf dist",
    "vercel-build": "yarn typedoc --plugin typedoc-plugin-missing-exports src/index.ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.7.14",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "typedoc": "^0.23.12",
    "typedoc-plugin-markdown": "^3.13.5",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@onehop/json-methods": "^1.1.2",
    "isomorphic-fetch": "^3.0.0",
    "zod": "^3.18.0"
  }
}